            }, 1000);

            $(`[name='Price`).on('keyup', function () {
                debugger;
                if ($(`[name='Price`).val() != "") {
                    //$('#subPrice').val($(`[name='Price`).val());
                    $('[id=subPrice]').each(function () {
                        $(this).val($(`[name='Price`).val());
                    })
                } else {
                    $('[id=subPrice]').each(function () { $(this).val(0) })
                }


