                <!--end::Label-->
                <!--begin::Input-->
                <input id="kt_ecommerce_add_product_tags" name="kt_ecommerce_add_product_tags" class="form-control mb-2" value="" />
                <!--end::Input-->
                <!--begin::Description-->
                <div class="text-muted fs-7">Add tags to a product.</div>*@
                <!--end::Description-->
                <!--end::Input group-->
            </div>
            <!--end::Card body-->

        </div>
        <!--end::Category & tags-->
