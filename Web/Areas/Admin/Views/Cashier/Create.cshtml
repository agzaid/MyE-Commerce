            <div class="card-body pt-0">
                <!--begin::Input group-->
                <div class="mb-10 fv-row">
                    <!--begin::Label-->
                    <label class="required form-label" asp-for="Name"></label>
                    <!--end::Label-->
                    <!--begin::Input-->
                    <input type="text" asp-for="Name" class="form-control mb-2" placeholder="Product name" />
                    <!--end::Input-->
                    <!--begin::Description-->
                    <span asp-validation-for="Name" class="text-danger"></span>
                    <!--end::Description-->
                </div>
                <!--end::Input group-->
                <!--begin::Input group-->
                <div class="mb-10 fv-row fv-plugins-icon-container fv-plugins-bootstrap5-row-invalid">
                    <!--begin::Label-->
                    <label asp-for="ShortDescription" class="form-label"></label>
                    <!--end::Label-->
                    <!--begin::Editor-->
                    <input type="text" asp-for="ShortDescription" class="form-control mb-2" placeholder="Short Description" />
                    <!--end::Input-->
                    <!--begin::Description-->
                    <span asp-validation-for="ShortDescription" class="text-danger"></span>

                    <div id="lang-ShortDescription">
                        @Html.Raw(Model.ShortDescription)
                    </div>
                    <!--end::Editor-->

                </div>
                <!--end::Input group-->
                <div class="mb-10 fv-row fv-plugins-icon-container fv-plugins-bootstrap5-row-invalid">
                    <!--begin::Label-->
                    <label asp-for="Price" class="form-label required"></label>
                    <!--end::Label-->
                    <!--begin::Editor-->
                    <input type="number" asp-for="Price" class="form-control mb-2" value="" />
                    <!--end::Input-->
                    <!--begin::Description-->
                    <span asp-validation-for="Price" class="text-danger"></span>

                    <!--end::Editor-->

                </div>
                <!--end::Input group-->
            </div>
            <!--end::Card header-->
            <!--begin::Card body-->
            <div class="card-body pt-0 pb-0">
                <!--begin::Input group-->
                <!--begin::Label-->
                <label class="form-label">Categories</label>
                <!--end::Label-->
                <!--begin::Select2-->
                <select class="form-select mb-2" data-control="select2" data-placeholder="Select an option" data-allow-clear="true" multiple="multiple">
                    <option></option>
                    <option value="Computers">Computers</option>
                    <option value="Watches">Watches</option>
                    <option value="Headphones">Headphones</option>
                    <option value="Footwear">Footwear</option>
                    <option value="Cameras">Cameras</option>
                    <option value="Shirts">Shirts</option>
                    <option value="Household">Household</option>
                    <option value="Handbags">Handbags</option>
                    <option value="Wines">Wines</option>
                    <option value="Sandals">Sandals</option>
                </select>
                <!--end::Select2-->
                <!--begin::Description-->
                <div class="text-muted fs-7 mb-7">Add product to a category.</div>
                <!--end::Description-->
                <!--end::Input group-->
                <!--begin::Button-->
                <a asp-action="Create" asp-controller="Category" class="btn btn-light-primary btn-sm mb-10">
                    <!--begin::Svg Icon | path: icons/duotune/arrows/arr087.svg-->
                    <span class="svg-icon svg-icon-2">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                            <rect opacity="0.5" x="11" y="18" width="12" height="2" rx="1" transform="rotate(-90 11 18)" fill="black" />
                            <rect x="6" y="11" width="12" height="2" rx="1" fill="black" />
